      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here


var pontos = str2num(getVariable("pontos",""));
var vitorias = str2num(getVariable("vitorias",""));
var derrotas = str2num(getVariable("derrotas",""));
var empates = str2num(getVariable("empates",""));
var golos_marcados = str2num(getVariable("golos_marcados",""));
var golos_sofridos = str2num(getVariable("golos_sofridos",""));
var diferenca_golos = str2num(getVariable("diferenca_golos",""));
var jogos = str2num(getVariable("jogos",""));
var name = getVariable("name","");


if(estado == 'Terminado'){
	if(equipa_casa == name || equipa_fora == name){
		//Alert(name + " - " + equipa_vencedora + " - " + estado + " - " + pontos);
		jogos++;
		if(equipa_vencedora == "EMPATE"){
			pontos = pontos + 1;
			empates++;
		}
		else if(equipa_vencedora == name){
			pontos = pontos + 3;
		}
		if(equipa_casa == name){
			if(golos_casa > golos_fora){
				vitorias++;
			}
			else if(golos_fora > golos_casa){
				derrotas++;
			}
			golos_marcados += golos_casa;
			golos_sofridos += golos_fora;
		}
		else{
			if(golos_casa > golos_fora){
				derrotas++;
			}
			else if(golos_fora > golos_casa){
				vitorias++;
			}
			golos_marcados += golos_fora;
			golos_sofridos += golos_casa;
		}
	}
}

diferenca_golos = golos_marcados - golos_sofridos;

//Set variables
setVariable("vitorias",vitorias,"g");
setVariable("derrotas",derrotas,"g");
setVariable("empates",empates,"g");
setVariable("golos_marcados",golos_marcados,"g");
setVariable("golos_sofridos",golos_sofridos,"g");
setVariable("diferenca_golos",diferenca_golos,"g");
setVariable("jogos",jogos,"g");
setVariable("pontos",pontos,"g");</jsScript_script>
      </jsScript>
