    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here

var tempResultado;
var tempEquipa_Casa;
var tempEquipa_Fora;
var data;
var estado;
var resultado;
var resultado_fora;
var jornada;
var equipa_casa;
var equipa_fora;
var equipa_vencedora;


//Parse Json's
tempEquipa_Casa = JSON.parse(equipa_casa);
tempEquipa_Fora = JSON.parse(equipa_fora);
tempResultado = JSON.parse(resultado);

//Data transformation
equipa_casa = tempEquipa_Casa.name;
equipa_fora = tempEquipa_Fora.name;
if(estado == 'FINISHED')	
{
	resultado = tempResultado.fullTime.homeTeam;
	resultado_fora = tempResultado.fullTime.awayTeam;
}
else
{
	resultado = null;
	resultado_fora = null;
}

if(tempResultado.winner == 'AWAY_TEAM' &amp;&amp; estado == 'FINISHED'){
	equipa_vencedora = tempEquipa_Fora.name;
}
else if(tempResultado.winner == 'HOME_TEAM' &amp;&amp; estado == 'FINISHED'){
	equipa_vencedora = tempEquipa_Casa.name;
}
else if(tempResultado.winner == 'DRAW' &amp;&amp; estado == 'FINISHED'){
	equipa_vencedora = "EMPATE";
}
else{
	equipa_vencedora = null;
}

if(estado == 'FINISHED'){
	estado = 'Terminado';
}
else{
	estado = 'N達o jogado';
}</jsScript_script>
